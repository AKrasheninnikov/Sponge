      Sequence="250"
      ImageUrl="_layouts/images/Sponge/logo.png"
      Description="Sponge Framework Administration Site">
    <UrlAction Url="/Sponge/default.aspx"/>
  </CustomActionGroup>
  <CustomAction
    Id="Sponge.UI.CentralAdmin.ConfigurationItems"
    GroupId="Sponge.UI.CentralAdmin"
    Location="Microsoft.SharePoint.Administration.Default"
    Title="Config Items"
    Description="Config Items">
    <UrlAction Url="/Sponge/Lists/ConfigItems"/>
  </CustomAction>
